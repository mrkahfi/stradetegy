    <header id="header" class="full-header">

        <div id="header-wrap">

            <div class="container clearfix">

                <div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

                <!-- Logo
                ============================================= -->
                <div id="logo">
                    <a href="{{ path("dashboard_member") }}" class="standard-logo"
                    data-dark-logo="{{ asset('/bundles/jariffproject/frontend/images/logo.png') }}"><img
                    src="{{ asset('/bundles/jariffproject/frontend/images/logo.png') }}"
                    alt="TRADEtegy Logo"></a>
                    <a href="{{ path("dashboard_member") }}" class="retina-logo"
                    data-dark-logo="{{ asset('/bundles/jariffproject/frontend/images/logo-dark@2x.png') }}"><img
                    src="{{ asset('/bundles/jariffproject/frontend/images/logo-dark@2x.png') }}"
                    alt="TRADEtegy Logo"></a>
                </div>
                <!-- #logo end -->


                <!-- Top Search
                ============================================= -->
                {#<div id="top-search">#}
                {#<a href="#" id="top-search-trigger"><i class="icon-search3"></i><i class="icon-line-cross"></i></a>#}
                {#<form action="search.html" method="get">#}
                {#<input type="text" name="q" class="form-control" value="" placeholder="Type &amp; Hit Enter..">#}
                {#</form>#}
                {#</div><!-- #top-search end -->#}


                <!-- Primary Navigation
                ============================================= -->
                <nav id="primary-menu" class="style-2 sub-title">
                    <div class="container clearfix">
                        <ul>
                            <li><a href="{{ path("dashboard_member") }}">
                                <div>Home</div>
                            </a></li>
                            <li><a href="{{ path("member_search_shipments") }}">
                                <div>Search</div>
                            </a></li>
                            <li><a href="{{ path("profile_edit") }}">
                                <div>My Account</div>
                            </a></li>
                            <li><a href="{{ path("support_ticket_inbound_form") }}">
                                <div>Customer Service</div>
                            </a></li>
                            <li><a href="{{ path("member_logout") }}">
                                <div>Logout</div>
                            </a></li>

                        </ul>
                    </div>
                </nav>
                <!-- #primary-menu end -->

            </div>

        </div>


    </header>
    <!-- #header end -->

    

    <section id="content" style="margin-top: 10px;overflow: visible">

        <div class="container clearfix">

            {% block flashes %}
                {% for flashMessage in app.session.flashbag.get('success') %}
                    <div class="alert alert-success" role="alert">
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('error') %}
                    <div class="alert alert-danger" role="alert">
                        {{ flashMessage }}
                    </div>
                {% endfor %}
            {% endblock flashes %}


            {% block menu %}
            {% endblock menu %}

            {% block content %}

            {% endblock %}
        </div>


    </section>


    <!-- #page-title end -->

    <!-- Content
    ============================================= -->


    <!-- Content
    ============================================= -->
    <section id="content">
        <div class="content-wrap">
            <div class="container clearfix">

                <!-- Posts
                ============================================= -->
                <div id="posts" class="post-grid clearfix">


                </div>
                <!-- #posts end -->

            </div>
        </div>
    </section>
    <!-- #content end -->

    <!-- Sub footer -->


    <!-- Footer
    ============================================= -->


</div>
<!-- #wrapper end -->

<!-- Go To Top
    ============================================= -->
