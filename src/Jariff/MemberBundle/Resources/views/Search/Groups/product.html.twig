{% block style %}

    <link href="{{ asset('/bundles/jariffproject/frontend/hover/css/hoveralls.css') }}" type="text/css" rel="stylesheet"
          media="screen"/>

    <link href="{{ asset('/bundles/jariffproject/frontend/css-old/wrapping/wrap-ribbon.css') }}" type="text/css"
          rel="stylesheet"
          media="screen"/>

    <link href="{{ asset('/bundles/jariffproject/frontend/css-old/bootstrap.css') }}" type="text/css"
          rel="stylesheet"
          media="screen"/>

    <link href="{{ asset('/bundles/jariffproject/frontend/css-old/bootstrap-tour.css') }}" type="text/css"
          rel="stylesheet"
          media="screen"/>

    <style>
        .nav-tabs > li .close {
            margin: -3px 0 0 10px;
            font-size: 18px;
            padding: 5px 0;
            float: right;
        }

        .nav-tabs > li a[data-toggle=tab] {
            float: left !important;
        }
    </style>

{% endblock %}

{% block javascript %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

    <script src="{{ asset('bundles/jariffproject/frontend/js-old/bootstrap/bootstrap.js') }}"
            type="text/javascript"></script>

    <script src="{{ asset('/bundles/jariffproject/frontend/js-old/bootstrap-tour/bootstrap-tour.js') }}"
            type="text/javascript"></script>

    <script src="{{ asset('/bundles/jariffproject/frontend/js-old/jqBootstrapValidation.js') }}"
            type="text/javascript"></script>


    <script type="text/javascript">
        $(document).ready(function () {
            $.ajax({
                type: 'GET',
                url: Routing.generate('ajax_member_cache_group_searching', {category: 'product',s_cache:'{{ app.request.get('s_cache') }}'}),
                beforeSend: function () {
                    $('#product').html('<div style="margin-top:20px;text-align: center">' +
                            '<img src="/bundles/jariffproject/frontend/images-old/301.GIF">' +
                            '<h5 style="margin-top: 15px">Loading...</h5>' +
                            '</div>');
                },
                success: function (data) {
                    $('#product').html(data);
                }
            })
        })
    </script>
    {% block js_call %}



    {% endblock %}

{% endblock %}
