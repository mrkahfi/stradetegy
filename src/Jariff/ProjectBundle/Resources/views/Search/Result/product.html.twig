    {#src="{{ asset('bundles/jariffproject/frontend/images/logo2.png') }}"#}
    {#width="170" height="170" style=""></a>#}

    <h1 class="header-search-trade product-header">
        {#<img#}
        {#src="{{ asset('/bundles/jariffproject/frontend/images/flags-iso/shiny/48/'~res.getTransformed.getValue.getCountry|twolettercode~".png") }}">#}

        <span class="imartist">
                <a href="javascript:void(0);" style="color: #BC3415" class="j-add-tab-products"
                   data-url="{{ path('member_cache_group_searching',{'category':'product','slug' : res.getTransformed.getSlugShipperName,'s_cache':app.request.get('s_cache') }) }}">
                    {% set match = res.getResult.getHit["highlight"] %}
                    {{ match['product_desc'][0]|firstnlastword|raw }}</strong></strong>

                </a>
            </span>
        <br/>
        <span>Imported From {{ res.getTransformed.getSlugCountry|twolettercodecountry }} <br/>
            <n>Sent by Company {{ res.getTransformed.getShipperNamePr }}</n>
        </span>

    </h1>
    <input type="hidden" class='j-body-products' value="{{ match['product_desc'][0]|descfirstnlastword }}">
                <span class="charts-p">
                    <span class="imname">
                        <br/>

                        {{ match['product_desc'][0]|descfirstnlastword|raw }}</strong></strong>
                        <br/>
                        {#{% set key = res.getTransformed.getValue.getCompanyJoin|length - 1 %}#}
                        {#{% set companyjoin = res.getTransformed.getValue.getCompanyJoin[key] %}#}

                        Purchased by {{ res.getTransformed.getConsigneeNamePr }}, USA
                        {#{% if companyjoin.getContainerCount > 0 %}#}
                        {#{{ companyjoin.getContainerCount }} container(s)#}
                        {#{% else %}#}
                        Gross weight : {{ res.getTransformed.getTotalWeight|setting_gross_weight(app.user) }}
                        {#{% endif %}#}


                    </span>
                </span>

        <span class="image_rollover">
            <a href="javascript:void(0)" class="j-add-tab-product"></a>
            <a target="_blank"
               href="{{ path('member_importer_detail',{'slug':res.getTransformed.getSlugShipperName,'header':match['product_desc'][0]|firstnlastword,'body':match['product_desc'][0]|descfirstnlastword}) }}"></a>
        </span>

</div>
