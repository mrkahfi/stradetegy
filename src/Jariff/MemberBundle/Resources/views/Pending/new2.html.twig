        updatePrice();
        validatePIF();
        updateCC();
        validateBankwire();
        everything();
        {% endif %}
        {% if entity.customPlan == "true" %}
        updatePrice();
        validatePIF();
        updateCC();
        validateBankwire();
        custom();
        {% endif %}
    });

</script>
{% endblock javascript %}

