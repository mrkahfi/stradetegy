<div class="entry-content project_content portfolio_container three_blocks j-append-condition"
     style="margin-top: -25px;">
    <h5 style="margin-left: 15%;margin-top: 2%;">Advanced search is available for <strong>subscribers only</strong>. <a href="{{ path('signup') }}"><strong>See
                plans now
                Â»</strong></a></h5>

    <div class="cmsms_media">
        <div class="cmsms_media_box" style="width: 9%;">
            <div class="form_info cmsms_input">
                <select id="search_form_collect" name="condition[]" disabled="disabled"
                        required="required">
                    <option value="and">AND
                    </option>
                    <option value="or">OR
                    </option>
                    <option value="not">NOT
                    </option>
                </select>
            </div>
        </div>
    </div>
    <div class="cmsms_media">
        <div class="cmsms_media_box cmsms_media_box_overwrite" style="margin-left: -7%">
            <div class="form_info cmsms_input">
                <select id="search_form_collect" style="width: 83%" name="collect[]" disabled="disabled"
                        required="required">
                    <option value="all">All
                    </option>
                    <option value="importer_name"

                            >Importer Name
                    </option>
                    <option value="importer_address"
                            >Importer Address
                    </option>
                    <option value="exporter_name"

                            >Exporter Name
                    </option>
                    <option value="exporter_address"
                            >Exporter Address
                    </option>
                    <option value="value.companyjoin.notify_name">Importer Notify Party
                    </option>
                    <option value="value.companyjoin.us_port">US Port Shipment Entered
                    </option>

                    <option value="value.companyjoin.product_description">Product Description
                    </option>

                    <option value="value.companyjoin.carrier">Carrier Code
                    </option>
                    <option value="value.companyjoin.vessel">Vessel Name
                    </option>
                    <option value="value.companyjoin.foreign_port">Foreign Port
                    </option>
                    <option value="value.country">Origin Of Vessel
                    </option>
                    <option value="value.companyjoin.container_number">Container Number
                    </option>
                    <option value="value.companyjoin.bill_of_lading">Bill Of Lading Number
                    </option>

                </select>
            </div>
        </div>
    </div>
    <div class="cmsms_media">
        <div class="cmsms_media_box cmsms_media_box_overwrite_1" style="margin-left: -6%;">
            <div class="form_info cmsms_input">

                <input type="text" placeholder="Search" name="q[]" class="input_searching"
                       id="field_003" size="10" tabindex="11" required disabled="disabled"
                       value=""
                        />
            </div>
        </div>
    </div>
    <div class="cmsms_media">
        <div class="cmsms_media_box">
            <div class="form_info cmsms_input">
                <div class="button_click">
                    <a class="j-remove-append"
                       href="javascript:void(0)">
                        <img class="strip"
                             src="{{ asset('bundles/jariffproject/frontend/images/strip.jpg') }}"
                             width="18"></a>
                </div>
            </div>
        </div>
    </div>


</div>
{% block js_call %}
    <script type="text/javascript">
        $(document).ready(function () {
            $('.j-remove-append').on('click', function () {
                $(this).parent('.button_click').parent('.form_info').parent('.cmsms_media_box').parent('.cmsms_media').parent('.j-append-condition').remove();
            })
        })
    </script>
{% endblock %}