{#{% for res in resHybirdBuyer %}#}
<div class="span6 popresult {% if checkToChoice['importer_'~res.getTransformed.getSlugConsigneeName] is defined %}disable-choice{% endif %}">

    <div class="wrap-ribbon right-edge point lblue lblue-strade"><span class="strade">Importer</span></div>
    {#<a href="{{ path('member_importer_detail',{'slug':res.getTransformed.getValue.getSlugCompany}) }}" class="lightbox"#}
    {#title="Watch Lorde-Royals">#}
    {#<img#}
    {#class="popchart-img" alt=""#}
    {#src="{{ asset('bundles/jariffproject/frontend/images/logo2.png') }}"#}
    {#width="170" height="170" style=""></a>#}

    <h1 class="header-search-trade"><img
                src="{{ asset('/bundles/jariffproject/frontend/images/flags-iso/shiny/48/'~(res.getTransformed.getSlugCountry|upper)~".png") }}">

        <span class="imartist">
            <a href="javascript:void(0);" style="color: #BC3415" class="j-add-tab-importer"
               data-url="{{ path('member_cache_group_searching',{'category':'importer','slug' : res.getTransformed.getSlugConsigneeName,'s_cache':app.request.get('s_cache') }) }}">
                {{ res.getTransformed.getConsigneeName }}
            </a>


        </span><br/>
        <span>Importer From {{ res.getTransformed.getSlugCountryOriConsignee|twolettercodecountry }}</span>

    </h1>
        <span class="charts-p"><span class="imname">

        <br/>
                {#{% if res.getTransformed.getValue.getTotalContainerCount > 0 %}#}
                {#{{ res.getTransformed.getValue.getTotalContainerCount }}#}
                {#container(s) in#}
                {#{% else %}#}
                Gross weight {{ res.getTransformed.getTotalWeight|setting_gross_weight(app.user) }} in
                {#{% endif %}#}

                {#{% set key = res.getTransformed.getValue.getCompanyJoin|length - 1 %}#}
                {#{% set companyjoin = res.getTransformed.getValue.getCompanyJoin[key] %}#}

                {{ res.getTransformed.getTotalShipment }}
                time(s) imports from {{ res.getTransformed.getShipperName }}
                {%- if res.getTransformed.getTotalCompany > 1 %}
                    <a href="" style="color: #BC3415;">and more</a>
                {%- endif %}
                <br/>
                {#Exports to {{ res.getTransformed.getValue.getShipRegisteredIn }}#}
                {#: {{ res.getTransformed.getValue.getArrivalDate|date('Y-m-d')|compare_dates }}#}
                {#, Container to {{ res.getTransformed.getValue.getForeignPort }}#}


                from {{ res.getTransformed.getSlugCountryOriShipper|twolettercodecountry }}
                : {{ res.getTransformed.getActualArrivalDate|date("d-m-y")|compare_dates }}
                {#{% if res.getTransformed.getValue.getTotalContainerCount > 1 %}#}
                {#, Container to {{ companyjoin.getForeignPort }}#}
                {#{% endif %}#}

        </span>
        </span>

    <span class="image_rollover">
        <a href="javascript:void(0)" class="j-add-tab-import"
           alt="Making Tooltips has never been easier, now there's no excuse to leave them out."></a>
            <a target="_blank"
               href="{{ path('member_importer_detail',{'slug':res.getTransformed.getSlugConsigneeName}) }}"></a>
        </span>
</div>

{% block js_call %}

    <link href="{{ asset('/bundles/jariffproject/frontend/hover/css/hoveralls.css') }}" type="text/css"
          rel="stylesheet"
          media="screen"/>


    <script src="{{ asset('bundles/jariffproject/frontend/hover/js/jquery-1.7.1.min.js') }}"
            type="text/javascript"></script>

    <script src="{{ asset('bundles/jariffproject/frontend/hover/js/jquery.hoveralls.min.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('bundles/jariffproject/frontend/hover/js/jquery.easing.1.3.js') }}"
            type="text/javascript"></script>

    <script type="text/javascript">
        $(window).load(function () {
            $('.hoveralls_link').HoverAlls({tooltip: true, starts: "0px,50px", ends: "120px,-55px", returns: "0px,50px", bg_class: "tooltip1background", text_class: "tooltip1text", speed_in: 1000, speed_out: 380, effect_in: "easeOutBack", effect_out: "easeInSine", bg_width: "250px", bg_height: "135px"});
        })
    </script>
{% endblock %}